      </div>
    </section>
    <section class="tasks-section">
      <div class="task">
        <div class="task-details">
          <div class="task-title">
            <p>
              Take out clothes from washing machine
              <i class="bi bi-bookmark-x" id="canceled-icon"></i>
            </p>

            <i class="bi bi-three-dots-vertical"></i>
          </div>
          <p class="task-time">13th of May</p>
        </div>
        <div class="task-tags">
          <p>Home</p>
          <p>Urgent</p>
        </div>
      </div>
      <div class="task">
        <div class="task-details">
          <div class="task-title">
            <p>
              Work on HTML/CSS
              <i class="bi bi-bookmark-x" id="canceled-icon"></i>
            </p>

            <i class="bi bi-three-dots-vertical"></i>
          </div>
          <p class="task-time">17th of May</p>
        </div>
        <div class="task-tags">
          <p>Education</p>
          <p>Personal</p>
          <p>Urgent</p>
        </div>
      </div>
    </section>
  </body>
</html>
